<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragment1">
    <activity
        android:id="@+id/mainActivity"
        android:name="mk.webfactory.template.feature.main.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/fragment_blank1" />
    <fragment
        android:id="@+id/blankFragment1"
        android:name="mk.webfactory.template.feature.main.ui.BlankFragment1"
        android:label="fragment_blank1"
        tools:layout="@layout/fragment_blank1" >
        <action
            android:id="@+id/action_blankFragment1_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_blankFragment1_to_blankFragment2"
            app:destination="@id/blankFragment2" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment2"
        android:name="mk.webfactory.template.feature.main.ui.BlankFragment2"
        android:label="fragment_blank2"
        tools:layout="@layout/fragment_blank2" >
        <action
            android:id="@+id/action_blankFragment2_to_blankFragment1"
            app:destination="@id/blankFragment1" />
    </fragment>
</navigation>